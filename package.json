			"import": "./dist/squircle-lib.js",
			"require": "./dist/squircle-lib.umd.cjs"
		},
		"./svelte": {
			"types": "./src/components/Squircle.svelte.d.ts",
			"svelte": "./src/components/Squircle.svelte",
			"default": "./src/components/Squircle.svelte"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.2",
		"@types/node": "^20.11.0",
		"svelte": "5.0.0-next.272",
		"typescript": "~5.3.0",
		"vite": "^5.0.0",
		"vite-plugin-dts": "^3.7.0"
	},
	"dependencies": {
		"lit": "^3.1.0"
	}
