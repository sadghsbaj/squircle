{
	"name": "squircle-lib",
	"version": "0.0.3",
	"type": "module",
	"private": false,
	"files": [
		"dist",
		"src"
	],
	"main": "./dist/squircle-lib.umd.cjs",
	"module": "./dist/squircle-lib.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
